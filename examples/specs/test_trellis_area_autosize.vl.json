{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Stock prices of four large companies as a small multiples of area charts.",
  "transform": [{"filter": "datum.symbol !== 'GOOG'"}],
  "width": 500,
  "height": 200,
  "autosize": "fit",
  "data": {"url": "data/stocks.csv"},
  "facet": {
    "row": {
      "field": "symbol",
      "title": "Symbol",
      "type": "nominal"
    }
  },
  "spec": {
    "mark": "area",
    "encoding": {
      "x": {
        "field": "date",
        "type": "temporal",
        "title": "Time",
        "axis": {"grid": false}
      },
      "y": {
        "field": "price",
        "type": "quantitative",
        "title": "Price",
        "axis": {"grid": false}
      },
      "color": {
        "field": "symbol",
        "type": "nominal",
        "legend": null
      }
    }
  }
}
